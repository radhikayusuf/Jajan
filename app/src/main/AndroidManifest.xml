<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pkl.gits.jajan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Jajan"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetailBarang.DetailActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".Category.Category.CategoryActivity" />
        <activity android:name=".Category.SubCategory.SubCategoryActivity" />
        <activity android:name=".Category.DetailCategory.DetailCategoryActivity" />
        <activity android:name=".ListProduk.ListProdukActivity"></activity>
    </application>

</manifest>